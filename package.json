    "lite-server": "^2.2.0",
    "typescript": "^2.0.0",

    "http-server": "^0.9.0",
    "jasmine-core": "~2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-cli": "^1.0.0",
    "karma-htmlfile-reporter": "^0.3.1",
    "karma-jasmine": "^1.0.2",
    "rimraf": "^2.5.2",

    "@types/core-js": "^0.9.27",
    "@types/jasmine": "^2.2.28",
    "@types/node": "^6.0.35"
  }
